using System;
using System.ComponentModel.DataAnnotations;
using System.ComponentModel.DataAnnotations.Schema;

namespace Coldairarrow.Entity.HrAssessment
{
    /// <summary>
    /// hr_assessment_evaluation
    /// </summary>
    [Table("hr_assessment_evaluation")]
    public class hr_assessment_evaluation
    {

        /// <summary>
        /// id
        /// </summary>
        [Key, Column(Order = 1)]
        public String id { get; set; }

        /// <summary>
        /// 测评项目名称
        /// </summary>
        public String evaluation_name { get; set; }

        /// <summary>
        /// 测评模板id
        /// </summary>
        public String assessment_template_id { get; set; }

        /// <summary>
        /// 测评对象ID
        /// </summary>
        public String user_id { get; set; }

        /// <summary>
        /// 测评对象姓名
        /// </summary>
        public String user_name { get; set; }

        /// <summary>
        /// 测评结束时间
        /// </summary>
        public DateTime? completion_time { get; set; }


        /// <summary>
        /// 状态
        /// </summary>
        public String status { get; set; }

        /// <summary>
        /// 创建人
        /// </summary>
        public String create_user { get; set; }

        /// <summary>
        /// 创建时间
        /// </summary>
        public DateTime? created_time { get; set; }

        /// <summary>
        /// 修改人
        /// </summary>
        public String updated_user { get; set; }

        /// <summary>
        /// 修改时间
        /// </summary>
        public DateTime? updated_time { get; set; }

        /// <summary>
        /// 排序字段
        /// </summary>
        public Single? Sort { get; set; }

        /// <summary>
        /// 否已删除
        /// </summary>
        public Boolean Deleted { get; set; }
    }
}